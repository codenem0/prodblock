{ 
    "manifest_version": 3,
    "name": "prodblock",
    "version": "0.1",
    "description": "An extension to block domains and a journal for productivity.",
    "permissions": ["storage", "webNavigation", "tabs"],
    "host_permissions": ["*://*.twitter.com/*", "*://*.instagram.com/*", "*://*.reddit.com/*"],
    "background": {"service_worker": "background.js"}

}